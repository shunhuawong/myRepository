            .extend(c3.areaPlot())
            .extend(DualPlottable())
            .extend({

                /**
                 * Creates a function to determine the bottom of the area plot at a certain x position.
                 * @returns {Function}
                 */
                areaBottom: function () {
                    var y0 = this.y0();
                    return function (d) {
                        return y0(d);
                    };
                },

                /**
                 * Creates a function to determine the top of the area plot at a certain x position.
                 * @returns {Function}
                 */
                areaTop: function () {
                    var y1 = this.y1();
                    return function (d) {
                        return y1(d);
                    };
