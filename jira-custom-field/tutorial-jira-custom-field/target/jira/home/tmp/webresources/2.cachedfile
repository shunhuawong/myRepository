                    .classed('backing-segment', true)
                    .attr('width', EACH_WIDTH + 2)
                    .attr('height', EACH_HEIGHT + 2)
                    .attr('x', function(d, i) { return i * EACH_WIDTH + i * OFFSET - 1.25; })
                    .attr('y', -1.25)
                    .attr('fill', this.color())
                    .attr('rx', 3);

