                        if (!inDom) {
                            component.mouseenter(e);
                            inDom = true;
                        }
                        component.mousemove(e);
                    } else {
                        if (inDom) {
                            component.mouseout(e);
                            inDom = false;
                        }
                    }
                })
                .on('mouseleave', function(e) {
                    component.mouseout(e);
                });
