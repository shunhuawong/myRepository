            .extend(c3.withDimensions())
            .extend(_.once(initialise))
            .extend({
                mouseCoordinates: c3.prop(),
                domContext: c3.prop(),
                region: c3.prop(),
                mousedown: c3.event(),
                mouseup: c3.event(),
                mousemove: c3.event(),
