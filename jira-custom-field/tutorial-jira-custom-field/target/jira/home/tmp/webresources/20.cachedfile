            var toSaveIds = [];
            var toSaveFieldTypes = [];

            var toSave = [model];
            if (!model) {
                toSave = this.getFields().filter(function(model) {
                    return !model.getSaving() && model.getEditing() && model.isDirty();
                });
            } else if (!model.getEditing() || model.getSaving()) {
