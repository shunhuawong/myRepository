            this.linksToOtherIssues();
            this.linksToReorderSubtasks();
            this.viewOptionsForCurrentIssue();
        },

        linksToOtherIssues: function() {
            jQuery(this.el).on("simpleClick", "a.issue-link[data-issue-key]", _.bind(function (e) {
