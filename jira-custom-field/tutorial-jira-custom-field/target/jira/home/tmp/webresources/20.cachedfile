                if (col.length > 0) {
                    this.setSelected(col.last());
                }
            }
            else {
                var index = col.indexOf(selected);
                var nextIndex = (index + col.length - 1) % col.length;
                this.setSelected(col.at(nextIndex));
            }
        }
    };

