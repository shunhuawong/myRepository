    function getDialog() {
        if (!dialogView) {
            dialogView = new WatchersInlineDialogView({
                el: $('#inline-dialog-watchers').get(0)
            });
        }
        return dialogView;
    }

    function getView(collection) {
        if (collection.isReadOnly) {
            return WatchersReadOnlyView;
        } else if (collection.canBrowseUsers) {
            return WatchersView;
