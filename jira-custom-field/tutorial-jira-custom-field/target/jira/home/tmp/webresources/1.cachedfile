    padding: 5px;
    padding-left: 10px;
}

.project-creation-success > ul > li {
    padding-bottom: 5px;
}

.project-creation-success > ul > li > div.created-entity-status {
    display: inline;
    padding-right: 10px;
}

.project-creation-acknowledge-spinner {
    padding-right: 10px;
}

