.gadget .gadget-menu .aui-icon.maximize {background-position: -39px 0; }
.gadget .maximized .gadget-menu .aui-icon.maximize {background-position: -52px 0; }
.gadget .gadget-menu .aui-dd-trigger {background-position: -13px 0; }
