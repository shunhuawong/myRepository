                    /*eslint-disable no-alert*/
                    return window.confirm(message);
                    /*eslint-enable no-alert*/
                },
                ignoreDirtiness: false
            });

            var message = JIRA.DirtyForm.getDirtyWarning() || JIRA.Issue.getDirtyCommentWarning();
