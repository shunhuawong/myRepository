    /**
     * Handles adding an ajax-backed dialog to the tools menu
     */
    JIRA.Issues.IssueTableHeaderOperationsView = JIRA.Issues.BaseView.extend({

        initialize: function(options) {
            _.bindAll(this,
                "_onSearchSuccess",
                "_updateFilterPageResources");

            _.extend(this, options);
            this.initTools();
