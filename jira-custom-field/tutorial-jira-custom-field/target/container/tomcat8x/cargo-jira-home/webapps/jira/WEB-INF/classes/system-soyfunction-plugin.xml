<atlassian-plugin key="com.atlassian.jira.plugin.system.soyfunction" i18n-name-key="admin.soyfunction.plugin.name" name="Soy Function Plugin">
    <plugin-info>
        <description key="soyfunction.plugin.desc">JIRA's system soy functions.</description>
        <version>1.0</version>
        <application-version min="3.4" max="4.3"/>
        <vendor name="Atlassian" url="http://www.atlassian.com"/>
    </plugin-info>

    <soy-function key="feature-flag-enabled-soy-function"
                  class="com.atlassian.jira.config.feature.FeatureFlagFunction"/>
    <soy-function key="jira-help-url-soy-function"
                  class="com.atlassian.jira.soy.HelpUrlFunction"/>
    <soy-function key="jira-help-title-soy-function"
                  class="com.atlassian.jira.soy.HelpTitleFunction"/>
    <soy-function key="jira-help-desc-soy-function"
                  class="com.atlassian.jira.soy.HelpDescriptionFunction"/>

</atlassian-plugin>
