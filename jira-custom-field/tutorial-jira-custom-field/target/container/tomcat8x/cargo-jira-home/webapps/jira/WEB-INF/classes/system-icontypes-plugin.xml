<atlassian-plugin key="jira.icontypes" name="Icon Types Plugin">
    <plugin-info>
        <description>System Icon Types</description>
        <version>1.0</version>
        <application-version min="7.1" />
        <vendor name="Atlassian" url="http://www.atlassian.com"/>
    </plugin-info>

    <icon-type key="user-icon-type" class="com.atlassian.jira.plugin.icon.UserIconTypeDefinition" defaultFilename="Avatar-default.svg">
    </icon-type>

    <icon-type key="issuetype-icon-type" class="com.atlassian.jira.plugin.icon.IssueTypeIconTypeDefinition" defaultFilename="genericissue.svg">
    </icon-type>

    <icon-type key="project-icon-type" class="com.atlassian.jira.plugin.icon.ProjectIconTypeDefinition" defaultFilename="default.svg">
    </icon-type>
</atlassian-plugin>
